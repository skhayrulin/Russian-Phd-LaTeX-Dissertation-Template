@article{CHKLOVSKII2010667,
  title = {Semi-automated reconstruction of neural circuits using electron microscopy},
  journal = {Current Opinion in Neurobiology},
  volume = {20},
  number = {5},
  pages = {667 - 675},
  year = {2010},
  note = {Neuronal and glial cell biology – New technologies},
  issn = {0959-4388"},
  doi = {https://doi.org/10.1016/j.conb.2010.08.002},
  url = {http://www.sciencedirect.com/science/article/pii/S0959438810001224},
  author = {Dmitri B Chklovskii and Shiv Vitaladevuni and Louis K Scheffer},
  abstract = {Reconstructing neuronal circuits at the level of synapses is a central problem in neuroscience, and the focus of the nascent field of connectomics. Previously used to reconstruct the C. elegans wiring diagram, serial-section transmission electron microscopy (ssTEM) is a proven technique for the task. However, to reconstruct more complex circuits, ssTEM will require the automation of image processing. We review progress in the processing of electron microscopy images and, in particular, a semi-automated reconstruction pipeline deployed at Janelia Farm. Drosophila circuits underlying identified behaviors are being reconstructed in the pipeline with the goal of generating a complete Drosophila connectome.}
}

